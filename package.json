{
  "name": "sms-shopify-rescript-build-system",
  "version": "2.0.0",
  "scripts": {
    "build:res": "bsb -make-world",
    "build:css": "NODE_ENV=production ./node_modules/.bin/postcss ./src/index.css -o ./theme/assets/index.css --verbose",
    "build": "run-s build:css build:res",
    "dev:res": "bsb -make-world -w",
    "dev:css": "NODE_ENV=production ./node_modules/.bin/postcss ./src/index.css -o ./theme/assets/index.css -w",
    "dev:css:old": "./node_modules/.bin/postcss ./src/index.css -o ./theme/assets/index.css -w",
    "clean": "bsb -clean-world",
    "dev": "./local.sh watch development",
    "dev:pull": "./local.sh pull development",
    "dev:deploy": "./local.sh deploy development",
    "stage:watch": "./local.sh watch staging",
    "stage:pull": "./local.sh pull staging",
    "stage:deploy": "./local.sh deploy staging",
    "prod:watch": "./local.sh watch production",
    "prod:pull": "./local.sh pull production",
    "prod:deploy": "./local.sh deploy production"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Logan Call @ SeaMonster Studios",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.2.3",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "^0.9.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "bs-platform": "^8.4.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}